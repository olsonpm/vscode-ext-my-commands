{
    "name": "my-commands",
    "displayName": "my-commands",
    "description": "my personal collection of commands",
    "publisher": "microsoft_thinks_this_is_mandatory",
    "version": "0.1.0",
    "engines": {
        "vscode": "^1.28.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:personal.hello"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "personal.hello",
                "title": "personal.hello"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "vscode": "^1.1.21"
    }
}
